{
  "expo": {
    "name": "gainz",
    "slug": "gainz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gainz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rasmus.gainz",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app needs camera access to scan food and log entries.",
        "NSMicrophoneUsageDescription": "This app needs microphone access for video recording.",
        "NSHealthShareUsageDescription": "This app needs access to your Health data to read workouts and nutrition information.",
        "NSHealthUpdateUsageDescription": "This app needs permission to write your workouts and nutrition data to Health.",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "UIApplicationShortcutItems": [
          {
            "UIApplicationShortcutItemType": "log-food",
            "UIApplicationShortcutItemTitle": "Log Food",
            "UIApplicationShortcutItemSubtitle": "Quick food entry",
            "UIApplicationShortcutItemIconType": "UIApplicationShortcutIconTypeAdd"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rasmus.gainz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-health-kit",
      "expo-health-connect",
      "expo-localization",
      "expo-font",
      "expo-quick-actions",
      "react-native-vision-camera",
      "expo-web-browser",
      [
        "react-native-health",
        {
          "NSHealthShareUsageDescription": "This app needs access to read your health data to track your daily activity.",
          "NSHealthUpdateUsageDescription": "This app needs permission to write health data to your HealthKit store."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "acb5a7ba-aa65-4853-a49d-352a159e8b91"
      }
    }
  }
}